    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Arrow icon retrieved from:"
        android:id="@+id/textView"
        android:textColor="#fff"
        android:textSize="15sp"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/about_link6"
        android:layout_alignStart="@+id/textView2" />

    <!-- Link7 -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http://www.flaticon.com/free-icon/right-arrow_118740#term=arrow&amp;page=1&amp;currentPosition=1"
        android:id="@+id/textView2"
        android:textColor="#fff"
        android:textSize="15sp"
        android:autoLink="all"
        android:clickable="true"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/about_link6" />


</RelativeLayout>